<Window x:Class="ProjektMonitoringNuget.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektMonitoringNuget"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="0,0,0,-228">
            <DataGrid x:Name="txtBox" Width="500" Height="500" ItemsSource="{Binding Result}"></DataGrid>

            <TextBox Width="150" Height="50" Name="txtRes"></TextBox>
            <Button x:Name="cmdTest" Width="150" Height="50" Content="Test" Click="cmdTest_Click"></Button>
        </StackPanel>
        
    </Grid>
</Window>
